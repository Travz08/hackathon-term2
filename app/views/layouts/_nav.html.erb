        </div>
    <% else %>
      <ul class="navbar-nav ml-auto">

        <li class= "nav-item"><%= link_to image_tag('heart.svg'), favorite_puppy_path(current_user) %></li>
        <!-- <li class= "nav-item"><%#= link_to image_tag('inbox.svg'), message_path %></li> -->
        <li class= "nav-item"><%= link_to image_tag('profile.svg'), edit_user_registration_path %></li>
        <li class= "nav-item"><%= link_to image_tag('logout.svg'), destroy_user_session_path, method: :delete %></li>
